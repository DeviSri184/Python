def natural_numbers(n):
    if n==0:
        return 0
    natural_numbers(n-1)
    print(n)

if __name__ == '__main__':
    n=int(input())
    natural_numbers(n)      


"""sum of n natural numbers"""
n=int(input())
def sum_of_n(n):
    if n==0:
        return 0
    return n+sum_of_n(n-1)
print(sum_of_n(n))
